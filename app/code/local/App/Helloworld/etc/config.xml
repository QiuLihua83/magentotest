<?xml version="1.0" encoding="UTF-8"?>

<config>

    <modules>
        <App_Helloworld>
            <version>0.1.0</version>
            
        </App_Helloworld>
    </modules>
    
    <frontend>
         <routers>
             <helloworld>
                 <use>standard</use>
                 <args>
                     <module>App_Helloworld</module>
                     <frontName>helloworld</frontName>
                 </args>
             </helloworld>
         </routers>
    </frontend>

    <global>
        <models>
            <helloworld>
                <class>App_Helloworld_Model</class>
                <resourcemodel>helloworld_mysql4</resourcemodel>
            </helloworld>
        </models>
    </global>

</config>